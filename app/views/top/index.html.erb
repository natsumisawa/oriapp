<!-- portfolio grid section -->
<section id="portfolio">
  <div class="container">
    <div class="row">
      <% @img.each do |ele| %>
        <div class="col-sm-6 col-xs-3 portfolio-item">
          <img src=<%= ele.img_url %> alt="">
        </div>
